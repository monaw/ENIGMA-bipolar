{
    global $user;
    global $EBD_DEBUG_FILE;
    global $EBD_MASTER_FILENAME;
    global $EBD_PRIVATE_URI_PREFIX;

    //$debug = fopen ( $EBD_DEBUG_FILE, "a" );
