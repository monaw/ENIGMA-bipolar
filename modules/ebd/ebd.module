    if ( $uri == $EBD_PRIVATE_URI_PREFIX . $EBD_MASTER_FILENAME &&
        user_has_role ( 5 ) )
    {
        $mimetype = file_get_mimetype ( $uri );
        //fwrite ( $debug, "mimetype : " . print_r ( $mimetype, TRUE ) . "\n" );

        return ( array ( 'Content-Type' => $mimetype ) );
    }
    else
        return ( NULL );
}


